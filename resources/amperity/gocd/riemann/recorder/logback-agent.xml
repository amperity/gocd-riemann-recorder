<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <contextName>go-agent</contextName>

  <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%date{ISO8601} %-20([%thread]) %-5level %-30logger{30}  %msg%n</pattern>
    </encoder>
  </appender>

  <root level="INFO">
    <appender-ref ref="console"/>
  </root>
</configuration>
